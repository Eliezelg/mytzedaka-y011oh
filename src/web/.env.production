# API Configuration
REACT_APP_API_URL=https://api.production.ijap.org
REACT_APP_API_VERSION=v1
REACT_APP_API_TIMEOUT=30000
REACT_APP_RATE_LIMIT_REQUESTS=100
REACT_APP_RATE_LIMIT_INTERVAL=60000

# Payment Gateway Configuration
REACT_APP_STRIPE_PUBLIC_KEY=pk_live_stripe_key
REACT_APP_STRIPE_ACCOUNT_TYPE=connect
REACT_APP_TRANZILLA_TERMINAL_KEY=live_terminal_key
REACT_APP_TRANZILLA_API_URL=https://secure.tranzilla.com/api

# Storage and WebSocket Configuration
REACT_APP_STORAGE_URL=https://storage.production.ijap.org
REACT_APP_WEBSOCKET_URL=wss://ws.production.ijap.org
REACT_APP_MAX_UPLOAD_SIZE=10485760

# Error Tracking and Monitoring
REACT_APP_SENTRY_DSN=https://sentry.production.ijap.org
REACT_APP_SENTRY_ENVIRONMENT=production
REACT_APP_GA_TRACKING_ID=UA-PRODUCTION-ID
REACT_APP_GA_DEBUG_MODE=false

# Security Configuration
REACT_APP_RECAPTCHA_SITE_KEY=production_recaptcha_site_key
REACT_APP_RECAPTCHA_VERSION=v3
REACT_APP_ENCRYPTION_KEY_VERSION=v1

# Build Configuration
NODE_ENV=production
GENERATE_SOURCEMAP=false